{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "e-wally-app-espace": {
      "root": "projects/e-wally-app-espace/",
      "sourceRoot": "projects/e-wally-app-espace/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "backend/dist/client",
            "index": "projects/e-wally-app-espace/src/index.html",
            "main": "projects/e-wally-app-espace/src/main.ts",
            "polyfills": "projects/e-wally-app-espace/src/polyfills.ts",
            "tsConfig": "projects/e-wally-app-espace/tsconfig.app.json",
            "assets": [
              "projects/e-wally-app-espace/src/favicon.ico",
              "projects/e-wally-app-espace/src/assets"
            ],
            "styles": [
              "projects/e-wally-app-espace/src/styles.css",
              "projects/e-wally-app-espace/src/assets/css/style.css",
              "projects/e-wally-app-espace/src/assets/css/bootstrap.css",
              "projects/e-wally-app-espace/src/assets/css/font-awesome.css",
              "projects/e-wally-app-espace/src/assets/css/ionicons.css",
              "projects/e-wally-app-espace/src/assets/css/plugin/sidebar-menu.css",
              "projects/e-wally-app-espace/src/assets/css/plugin/animate.css",
              "projects/e-wally-app-espace/src/assets/css/jquery-ui.css"
            ],
            "scripts": [
              "projects/e-wally-app-espace/src/assets/js/jquery-1.11.2.min.js",
              "projects/e-wally-app-espace/src/assets/js/jquery-ui.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.easing.js",
              "projects/e-wally-app-espace/src/assets/js/bootstrap.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.flexslider.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.fitvids.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.viewportchecker.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.stellar.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/wow.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.colorbox-min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/owl.carousel.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/isotope.pkgd.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/masonry.pkgd.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/imagesloaded.pkgd.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.fs.tipper.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/mediaelement-and-player.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/sidebar-menu.js",
              "projects/e-wally-app-espace/src/assets/js/theme.js",
              "projects/e-wally-app-espace/src/assets/js/navigation.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/e-wally-app-espace/src/environments/environment.ts",
                  "with": "projects/e-wally-app-espace/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "e-wally-app-espace:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "e-wally-app-espace:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "e-wally-app-espace:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/e-wally-app-espace/src/test.ts",
            "polyfills": "projects/e-wally-app-espace/src/polyfills.ts",
            "tsConfig": "projects/e-wally-app-espace/tsconfig.spec.json",
            "karmaConfig": "projects/e-wally-app-espace/karma.conf.js",
            "styles": [
              "projects/e-wally-app-espace/src/styles.css",
              "projects/e-wally-app-espace/src/assets/css/style.css",
              "projects/e-wally-app-espace/src/assets/css/bootstrap.css",
              "projects/e-wally-app-espace/src/assets/css/font-awesome.css",
              "projects/e-wally-app-espace/src/assets/css/ionicons.css",
              "projects/e-wally-app-espace/src/assets/css/plugin/sidebar-menu.css",
              "projects/e-wally-app-espace/src/assets/css/plugin/animate.css",
              "projects/e-wally-app-espace/src/assets/css/jquery-ui.css"
            ],
            "scripts": [
              "projects/e-wally-app-espace/src/assets/js/jquery-1.11.2.min.js",
              "projects/e-wally-app-espace/src/assets/js/jquery-ui.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.easing.js",
              "projects/e-wally-app-espace/src/assets/js/bootstrap.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.flexslider.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.fitvids.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.viewportchecker.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.stellar.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/wow.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.colorbox-min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/owl.carousel.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/isotope.pkgd.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/masonry.pkgd.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/imagesloaded.pkgd.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/jquery.fs.tipper.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/mediaelement-and-player.min.js",
              "projects/e-wally-app-espace/src/assets/js/plugin/sidebar-menu.js",
              "projects/e-wally-app-espace/src/assets/js/theme.js",
              "projects/e-wally-app-espace/src/assets/js/navigation.js"
            ],
            "assets": [
              "projects/e-wally-app-espace/src/favicon.ico",
              "projects/e-wally-app-espace/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/e-wally-app-espace/tsconfig.app.json",
              "projects/e-wally-app-espace/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "wally"
}
